<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Airbnb Test" parallel="methods"
	thread-count="2">
	<listeners>
		<listener
			class-name="reusableComponents.TestListeners" />
	</listeners>
	<test name="Test1">
	<parameter name="browser" value="chrome" />
		<classes>
			<class name="Tests.AirbnbTests" />
		</classes>
	</test> <!-- Test -->
	 <test name="Test2">
		<parameter name="browser" value="firefox" />
		<classes>
		<class name="Tests.AirbnbTests" /> 
		</classes>
	</test>  
</suite> <!-- Suite -->
